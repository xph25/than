-- âœˆï¸ é£›è¡ŒåŠŸèƒ½
createBtn("âœˆï¸ é£›è¡ŒåŠŸèƒ½", 55, Color3.fromRGB(60, 100, 160), function()
    local player = game.Players.LocalPlayer
    local uis = game:GetService("UserInputService")
    local runService = game:GetService("RunService")
    local tweenService = game:GetService("TweenService")
    
    if player.PlayerGui:FindFirstChild("FlyGui") then player.PlayerGui.FlyGui:Destroy() end

    -- === è®Šæ•¸è¨­å®š ===
    local flying = false
    local noclip = false
    local speed = 100
    local isMinimized = false
    local keys = {W = false, S = false, A = false, D = false, Space = false, LShift = false}
    local bv, bg

    -- === ç©¿ç‰†é‚è¼¯ ===
    runService.Stepped:Connect(function()
        if noclip and player.Character then
            for _, part in pairs(player.Character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = false
                end
            end
        end
    end)

    -- === UI æ§‹å»º ===
    local screenGui = Instance.new("ScreenGui", player.PlayerGui)
    screenGui.Name = "FlyGui"
    screenGui.ResetOnSpawn = false

    local mainFrame = Instance.new("Frame", screenGui)
    mainFrame.Size = UDim2.new(0, 220, 0, 220)
    mainFrame.Position = UDim2.new(0.05, 0, 0.4, 0)
    mainFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    mainFrame.Active = true
    mainFrame.Draggable = true
    mainFrame.ClipsDescendants = true
    Instance.new("UICorner", mainFrame)

    -- æ¨™é¡Œæ¬„ (ç”¨æ–¼æ‹–æ‹½èˆ‡æŽ§åˆ¶æŒ‰éˆ•)
    local titleBar = Instance.new("Frame", mainFrame)
    titleBar.Size = UDim2.new(1, 0, 0, 30)
    titleBar.BackgroundTransparency = 1

    local title = Instance.new("TextLabel", titleBar)
    title.Size = UDim2.new(1, -60, 1, 0)
    title.Position = UDim2.new(0, 10, 0, 0)
    title.Text = "é«˜ç´šé£›è¡ŒæŽ§åˆ¶"
    title.TextColor3 = Color3.new(1, 1, 1)
    title.TextXAlignment = Enum.TextXAlignment.Left
    title.BackgroundTransparency = 1

    -- æœ€å°åŒ–æŒ‰éˆ•
    local miniBtn = Instance.new("TextButton", titleBar)
    miniBtn.Text = "-"; miniBtn.Size = UDim2.new(0, 25, 0, 25); miniBtn.Position = UDim2.new(1, -55, 0, 2)
    miniBtn.BackgroundColor3 = Color3.fromRGB(60, 60, 70); miniBtn.TextColor3 = Color3.new(1, 1, 1)
    Instance.new("UICorner", miniBtn)

    -- é—œé–‰æŒ‰éˆ•
    local closeBtn = Instance.new("TextButton", titleBar)
    closeBtn.Text = "Ã—"; closeBtn.Size = UDim2.new(0, 25, 0, 25); closeBtn.Position = UDim2.new(1, -27, 0, 2)
    closeBtn.BackgroundColor3 = Color3.fromRGB(150, 50, 50); closeBtn.TextColor3 = Color3.new(1, 1, 1)
    Instance.new("UICorner", closeBtn)

    -- å…§å®¹å®¹å™¨ (ç”¨æ–¼æœ€å°åŒ–æ™‚éš±è—)
    local contentFrame = Instance.new("Frame", mainFrame)
    contentFrame.Size = UDim2.new(1, 0, 1, -30)
    contentFrame.Position = UDim2.new(0, 0, 0, 30)
    contentFrame.BackgroundTransparency = 1

    -- 1. é£›è¡Œé–‹é—œ
    local toggleBtn = Instance.new("TextButton", contentFrame)
    toggleBtn.Size = UDim2.new(0, 180, 0, 35); toggleBtn.Position = UDim2.new(0.5, -90, 0, 10)
    toggleBtn.Text = "é£›è¡Œ: é—œé–‰"; toggleBtn.BackgroundColor3 = Color3.fromRGB(60, 60, 60); toggleBtn.TextColor3 = Color3.new(1, 1, 1)
    Instance.new("UICorner", toggleBtn)

    -- 2. Noclip é–‹é—œ
    local noclipBtn = Instance.new("TextButton", contentFrame)
    noclipBtn.Size = UDim2.new(0, 180, 0, 35); noclipBtn.Position = UDim2.new(0.5, -90, 0, 50)
    noclipBtn.Text = "ç©¿ç‰†: é—œé–‰"; noclipBtn.BackgroundColor3 = Color3.fromRGB(60, 60, 60); noclipBtn.TextColor3 = Color3.new(1, 1, 1)
    Instance.new("UICorner", noclipBtn)

    -- 3. é€Ÿåº¦æŽ§åˆ¶
    local speedFrame = Instance.new("Frame", contentFrame)
    speedFrame.Size = UDim2.new(0, 180, 0, 35); speedFrame.Position = UDim2.new(0.5, -90, 0, 90); speedFrame.BackgroundTransparency = 1

    local minusBtn = Instance.new("TextButton", speedFrame)
    minusBtn.Text = "-"; minusBtn.Size = UDim2.new(0, 35, 1, 0); minusBtn.BackgroundColor3 = Color3.fromRGB(200, 50, 50); minusBtn.TextColor3 = Color3.new(1, 1, 1)
    Instance.new("UICorner", minusBtn)

    local speedLabel = Instance.new("TextBox", speedFrame)
    speedLabel.Text = tostring(speed); speedLabel.Size = UDim2.new(0, 100, 1, 0); speedLabel.Position = UDim2.new(0, 40, 0, 0)
    speedLabel.BackgroundColor3 = Color3.fromRGB(50, 50, 50); speedLabel.TextColor3 = Color3.new(1, 1, 1)
    Instance.new("UICorner", speedLabel)

    local plusBtn = Instance.new("TextButton", speedFrame)
    plusBtn.Text = "+"; plusBtn.Size = UDim2.new(0, 35, 1, 0); plusBtn.Position = UDim2.new(1, -35, 0, 0)
    plusBtn.BackgroundColor3 = Color3.fromRGB(50, 200, 50); plusBtn.TextColor3 = Color3.new(1, 1, 1)
    Instance.new("UICorner", plusBtn)

    -- === æ ¸å¿ƒåŠŸèƒ½å‡½æ•¸ ===
    local function setupPhysics(root)
        if root:FindFirstChild("FlyBV") then root.FlyBV:Destroy() end
        if root:FindFirstChild("FlyBG") then root.FlyBG:Destroy() end
        bv = Instance.new("BodyVelocity", root); bv.Name = "FlyBV"
        bv.MaxForce = Vector3.new(math.huge, math.huge, math.huge); bv.Velocity = Vector3.new(0, 0, 0)
        bg = Instance.new("BodyGyro", root); bg.Name = "FlyBG"
        bg.MaxTorque = Vector3.new(math.huge, math.huge, math.huge); bg.P = 15000
    end

    local function toggleFly()
        flying = not flying
        local char = player.Character
        local root = char and char:FindFirstChild("HumanoidRootPart")
        local hum = char and char:FindFirstChildOfClass("Humanoid")
        if not root or not hum then return end
        if flying then
            setupPhysics(root)
            hum.PlatformStand = true
            toggleBtn.Text = "é£›è¡Œ: é–‹å•Ÿ"; toggleBtn.BackgroundColor3 = Color3.fromRGB(0, 150, 0)
        else
            if bv then bv:Destroy() end; if bg then bg:Destroy() end
            hum.PlatformStand = false; hum:ChangeState(Enum.HumanoidStateType.GettingUp)
            toggleBtn.Text = "é£›è¡Œ: é—œé–‰"; toggleBtn.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        end
    end

    -- === äº‹ä»¶ç¶å®š ===
    miniBtn.MouseButton1Click:Connect(function()
        isMinimized = not isMinimized
        local targetSize = isMinimized and UDim2.new(0, 220, 0, 30) or UDim2.new(0, 220, 0, 220)
        tweenService:Create(mainFrame, TweenInfo.new(0.3), {Size = targetSize}):Play()
        contentFrame.Visible = not isMinimized
        miniBtn.Text = isMinimized and "+" or "-"
    end)

    toggleBtn.MouseButton1Click:Connect(toggleFly)
    noclipBtn.MouseButton1Click:Connect(function()
        noclip = not noclip
        noclipBtn.Text = "ç©¿ç‰†: " .. (noclip and "é–‹å•Ÿ" or "é—œé–‰")
        noclipBtn.BackgroundColor3 = noclip and Color3.fromRGB(0, 150, 0) or Color3.fromRGB(60, 60, 60)
    end)

    plusBtn.MouseButton1Click:Connect(function() speed = speed + 50; speedLabel.Text = tostring(speed) end)
    minusBtn.MouseButton1Click:Connect(function() speed = math.max(0, speed - 50); speedLabel.Text = tostring(speed) end)
    speedLabel.FocusLost:Connect(function() speed = tonumber(speedLabel.Text) or speed; speedLabel.Text = tostring(speed) end)
    closeBtn.MouseButton1Click:Connect(function() flying = false; noclip = false; screenGui:Destroy() end)

    -- === è¼¸å…¥èˆ‡ç‰©ç†å¾ªç’° ===
    uis.InputBegan:Connect(function(i, g)
        if g then return end
        local k = i.KeyCode.Name
        if keys[k] ~= nil then keys[k] = true end
        if i.KeyCode == Enum.KeyCode.G then toggleFly() end
        if i.KeyCode == Enum.KeyCode.Space then keys.Space = true end
        if i.KeyCode == Enum.KeyCode.LeftShift then keys.LShift = true end
    end)
    uis.InputEnded:Connect(function(i)
        local k = i.KeyCode.Name
        if keys[k] ~= nil then keys[k] = false end
        if i.KeyCode == Enum.KeyCode.Space then keys.Space = false end
        if i.KeyCode == Enum.KeyCode.LeftShift then keys.LShift = false end
    end)

    runService.RenderStepped:Connect(function()
        local char = player.Character
        local root = char and char:FindFirstChild("HumanoidRootPart")
        if flying and root and bv and bg then
            local cam = workspace.CurrentCamera
            local fwd = (keys.W and 1 or 0) - (keys.S and 1 or 0)
            local side = (keys.D and 1 or 0) - (keys.A and 1 or 0)
            local up = (keys.Space and 1 or 0) - (keys.LShift and 1 or 0)
            local dir = cam.CFrame:VectorToWorldSpace(Vector3.new(side, up, -fwd))
            bv.Velocity = dir.Magnitude > 0 and (dir.Unit * speed) or Vector3.new(0, 0, 0)
            bg.CFrame = cam.CFrame
        end
    end)
end)
