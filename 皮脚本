attackBtn.MouseButton1Click:Connect(function()
    attackOn = not attackOn
    attackBtn.Text = attackOn and "âš¡ å¿«é€Ÿæ”»æ“Š (é–‹å•Ÿ)" or "âš¡ å¿«é€Ÿæ”»æ“Š (é—œé–‰)"
    attackBtn.BackgroundColor3 = attackOn and Color3.fromRGB(50, 160, 50) or Color3.fromRGB(60, 80, 110)
    if not fastAttackSystem then fastAttackSystem = initializeFastAttack() end
    if fastAttackSystem then if attackOn then fastAttackSystem.start() else fastAttackSystem.stop() end end
end)


