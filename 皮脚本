-- ========================
-- 1. UI æ§‹å»º
-- ========================
local player = game.Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")
local UIS = game:GetService("UserInputService")
local RunService = game:GetService("RunService")

-- ========================
-- 1. UI ä»‹é¢è¨­å®š
-- ========================
local screenGui = Instance.new("ScreenGui", playerGui)
screenGui.Name = "Aimbot_Player_Only"
screenGui.ResetOnSpawn = false

local mainFrame = Instance.new("Frame", screenGui)
mainFrame.Size = UDim2.new(0, 220, 0, 120)
mainFrame.Position = UDim2.new(0.5, -110, 0.5, -60)
mainFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 30)
mainFrame.Active = true
mainFrame.Draggable = true 
Instance.new("UICorner", mainFrame)

-- RGB é‚Šæ¡†æ•ˆæžœ
local stroke = Instance.new("UIStroke", mainFrame)
stroke.Thickness = 2
RunService.RenderStepped:Connect(function()
    stroke.Color = Color3.fromHSV(tick() % 5 / 5, 0.8, 1)
end)

local title = Instance.new("TextLabel", mainFrame)
title.Text = "çŒªçŒªè‡ªçž„V1.1 | by XPh"
title.Size = UDim2.new(1, 0, 0, 35)
title.TextColor3 = Color3.new(1, 1, 1)
title.Font = Enum.Font.GothamBold
title.BackgroundTransparency = 1

-- ========================
-- 2. è‡ªçž„æ ¸å¿ƒé‚è¼¯
-- ========================
local aimbotEnabled = false
local aimPart = "HumanoidRootPart"

local function getClosestPlayer()
    local closest, dist = nil, 1000
    for _, v in pairs(game.Players:GetPlayers()) do
        if v ~= player and v.Character and v.Character:FindFirstChild(aimPart) then
            local hum = v.Character:FindFirstChildOfClass("Humanoid")
            if hum and hum.Health > 0 then
                local d = (player.Character.HumanoidRootPart.Position - v.Character[aimPart].Position).Magnitude
                if d < dist then
                    dist = d
                    closest = v.Character
                end
            end
        end
    end
    return closest
end

-- ========================
-- 3. æŒ‰éˆ•èˆ‡å¿«æ·éµ (å·²æ”¹ç‚º H)
-- ========================
local aimBtn = Instance.new("TextButton", mainFrame)
aimBtn.Size = UDim2.new(0, 180, 0, 45)
aimBtn.Position = UDim2.new(0.5, -90, 0, 50)
aimBtn.BackgroundColor3 = Color3.fromRGB(180, 60, 60)
aimBtn.Text = "éŽ–å®šçŽ©å®¶: é—œé–‰ (H)"
aimBtn.TextColor3 = Color3.new(1, 1, 1)
aimBtn.Font = Enum.Font.GothamMedium
Instance.new("UICorner", aimBtn)

local function updateUI()
    if aimbotEnabled then
        aimBtn.Text = "éŽ–å®šçŽ©å®¶: é–‹å•Ÿ"
        aimBtn.BackgroundColor3 = Color3.fromRGB(60, 180, 60)
    else
        aimBtn.Text = "éŽ–å®šçŽ©å®¶: é—œé–‰ (H)"
        aimBtn.BackgroundColor3 = Color3.fromRGB(180, 60, 60)
    end
end

-- åˆ‡æ›é–‹é—œé‚è¼¯
local function toggle()
    aimbotEnabled = not aimbotEnabled
    updateUI()
end

aimBtn.MouseButton1Click:Connect(toggle)

-- å¿«æ·éµ H ç›£è½
UIS.InputBegan:Connect(function(input, processed)
    if not processed and input.KeyCode == Enum.KeyCode.H then
        toggle()
    end
end)

-- åŸ·è¡Œå¾ªç’°
RunService.RenderStepped:Connect(function()
    if aimbotEnabled then
        pcall(function()
            local target = getClosestPlayer()
            if target and target:FindFirstChild(aimPart) then
                local cam = workspace.CurrentCamera
                cam.CFrame = CFrame.new(cam.CFrame.Position, target[aimPart].Position)
            end
        end)
    end
end)

-- é—œé–‰ UI æŒ‰éˆ•
local close = Instance.new("TextButton", mainFrame)
close.Text = "Ã—"
close.Size = UDim2.new(0, 25, 0, 25)
close.Position = UDim2.new(1, -30, 0, 5)
close.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
close.TextColor3 = Color3.new(1, 1, 1)
