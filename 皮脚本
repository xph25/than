-- ========================
-- 0. é€šç”¨å·¥å…·èˆ‡å‹•æ…‹æ•ˆæžœ
-- ========================
local function safeSetText(obj, text)
    if obj and typeof(obj) == "Instance" then
        pcall(function() obj.Text = text end)
    end
end

local function applyDynamicEffect(btn)
    local normalSize = btn.Size
    local hoverSize = UDim2.new(normalSize.X.Scale, normalSize.X.Offset + 6, normalSize.Y.Scale, normalSize.Y.Offset + 6)
    local tweenInfo = TweenInfo.new(0.2, Enum.EasingStyle.Back, Enum.EasingDirection.Out)
    
    btn.MouseEnter:Connect(function() TweenService:Create(btn, tweenInfo, {Size = hoverSize}):Play() end)
    btn.MouseLeave:Connect(function() TweenService:Create(btn, tweenInfo, {Size = normalSize}):Play() end)
end

-- ========================
-- 1. å¿«é€Ÿæ”»æ“Šé‚è¼¯
-- ========================
local fastAttackSystem = nil
local function initializeFastAttack()
    local function SafeWait(parent, name)
        local s, r = pcall(function() return parent:WaitForChild(name, 5) end)
        return s and r or nil
    end

    local RS = game:GetService("ReplicatedStorage")
    local Remotes = SafeWait(RS, "Remotes")
    local Net = SafeWait(RS:FindFirstChild("Modules") or {}, "Net")
    local RegisterAttack = Net and SafeWait(Net, "RE/RegisterAttack")
    local RegisterHit = Net and SafeWait(Net, "RE/RegisterHit")
    
    local attackLoop
    return {
        start = function()
            attackLoop = task.spawn(function()
                while task.wait(0.01) do
                    local list = {}
                    for _, folderName in pairs({"Enemies", "Characters"}) do
                        local f = workspace:FindFirstChild(folderName)
                        if f then
                            for _, v in pairs(f:GetChildren()) do
                                if v:FindFirstChild("Head") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                                    if v ~= player.Character and player:DistanceFromCharacter(v.Head.Position) < 5000 then
                                        table.insert(list, {v, v.Head})
                                    end
                                end
                            end
                        end
                    end
                    if #list > 0 and RegisterAttack and RegisterHit then
                        pcall(function()
                            RegisterAttack:FireServer(0)
                            RegisterHit:FireServer(list[1][2], list)
                        end)
                    end
                end
            end)
        end,
        stop = function() if attackLoop then task.cancel(attackLoop); attackLoop = nil end end
    }
end

